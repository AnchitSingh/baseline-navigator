{
  "name": "baseline-navigator",
  "displayName": "baseline-navigator",
  "description": "Smart Baseline compatibility with graph visualization",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.104.0"
  },
  "activationEvents": [
    "onLanguage:css",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:html"
  ],
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "baseline-navigator.showGraph",
        "title": "Baseline: Show Feature Graph"
      },
      {
        "command": "baseline-navigator.checkCompatibility",
        "title": "Baseline: Check File Compatibility"
      }
    ],
    "configuration": {
      "title": "Baseline Navigator",
      "properties": {
        "baselineNavigator.targetBrowsers": {
          "type": "array",
          "default": ["chrome >= 90", "firefox >= 88", "safari >= 14"],
          "description": "Target browsers for compatibility checking"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.104.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.36.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "webpack": "^5.102.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "d3": "^7.9.0",
    "d3-force": "^3.0.0",
    "web-features": "^3.3.0"
  }
}
